<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jjjjisun.coupangeats">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".config.ApplicationClass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CoupangEats"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".src.main.fragment.home.cart.CartActivity"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name=".src.main.fragment.home.menu.MenuActivity"
            android:screenOrientation="portrait">


        </activity>
        <activity
            android:name=".src.main.fragment.home.store.StoreActivity"
            android:label="@string/title_activity_store"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.CoupangEats.NoActionBar" />
        <activity
            android:name=".src.splash.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".src.login.Join.JoinActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".src.login.EmailLogin.EmailLoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".src.main.MainActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>